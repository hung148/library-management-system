<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400" prefWidth="300" stylesheets="@../pageStyle.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.main.controller.admin.AdminAccount">
     <GridPane styleClass="user-info-display" layoutX="50" layoutY="80" vgap="10.0">
         <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" prefWidth="100.0"/>
             <ColumnConstraints hgrow="SOMETIMES" prefWidth="150.0"/>
         </columnConstraints>
         <rowConstraints>
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints />
         </rowConstraints>
         <Label styleClass="accountLabels" text="Name:" />
         <Label styleClass="accountLabels" text="Username:" GridPane.rowIndex="1" />
         <Label prefHeight="17.0" prefWidth="44.0" styleClass="accountLabels" text="Email:" GridPane.rowIndex="2" />
         <Label fx:id="adminName" GridPane.columnIndex="1" />
         <Label fx:id="adminUserName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         <Label fx:id="adminEmail" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         <Button onAction="#onLogout" styleClass="signinPagebutton" text="Log out" GridPane.columnIndex="1" GridPane.rowIndex="3" />
     </GridPane>
</AnchorPane>
