<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300" prefWidth="400" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.main.controller.member.MemberAccount">
    
        <GridPane styleClass="user-info-display" alignment="CENTER" vgap="10.0" layoutX="50.0" layoutY="80.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" prefWidth="150.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <Label styleClass="accountLabels" text="Name:" />
            <Label styleClass="accountLabels" text="Username:" GridPane.rowIndex="1" />
            <Label prefHeight="17.0" prefWidth="44.0" styleClass="accountLabels" text="Email:" GridPane.rowIndex="2" />
            <Label fx:id="memberName" GridPane.columnIndex="1" />
            <Label fx:id="memberUserName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="memberEmail" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button styleClass="signinPagebutton" text="Log out" onAction="#onLogout" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
      <padding>
         <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
      </padding>
        </GridPane>
    
   
</AnchorPane>
